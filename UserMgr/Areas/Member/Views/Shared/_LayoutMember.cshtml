<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width" />
    <meta name="keywords" content="">
    <meta name="description" content="">
    <title>@ViewBag.Title</title>

    <link rel="shortcut icon" href="favicon.ico">
    <link href="~/jsscs/css/bootstrap.min.css?v=3.3.6" rel="stylesheet">
    <link href="~/jsscs/css/font-awesome.css?v=4.4.0" rel="stylesheet">
    <link href="~/jsscs/css/plugins/iCheck/custom.css" rel="stylesheet">
    <link href="~/jsscs/css/animate.css" rel="stylesheet">
    <link href="~/jsscs/css/style.css?v=4.1.0" rel="stylesheet">
    <script>if(window.top !== window.self){ window.top.location = window.location;}</script>
</head>
<body class="gray-bg">
    <div id="notiy">
        @if (TempData["Msg"] != null) 
        {
            <div class="alert alert-success alert-dismissable">
                <button aria-hidden="true" data-dismiss="alert" class="close" type="button">×</button>
                @TempData["Msg"].
            </div>
        }
    </div>


    @RenderBody()

    <!-- 全局js -->
    <script src="~/jsscs/js/jquery.min.js?v=2.1.4"></script>
    <script src="~/jsscs/js/bootstrap.min.js?v=3.3.6"></script>
</body>
</html>
